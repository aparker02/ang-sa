{
  "project": {
    "version": "1.0.0-beta.26",
    "name": "ang-sa"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico"
      ],
      "index": "index.html",
      "main": "main.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.json",
      "prefix": "app",
      "mobile": false,
      "styles": [
        "../node_modules/golden-layout/src/css/goldenlayout-base.css",
        "../node_modules/golden-layout/src/css/goldenlayout-dark-theme.css",
        "../src/assets/css/kendo.common.min.css",
        "../src/assets/css/kendo.default.min.css",
        "styles.css"
      ],
      "scripts": [
        "../node_modules/jquery/dist/jquery.js",
        "../node_modules/golden-layout/dist/goldenlayout.js",
        "../node_modules/amcharts/dist/amcharts/amcharts.js",
        "../node_modules/amcharts/dist/amcharts/serial.js",
        "../src/assets/js/kendo.all.min.js"
      ],
      "environments": {
        "source": "environments/environment.ts",
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "css",
    "prefixInterfaces": false,
    "inline": {
      "style": false,
      "template": false
    },
    "spec": {
      "class": false,
      "component": true,
      "directive": true,
      "module": false,
      "pipe": true,
      "service": true
    }
  }
}
